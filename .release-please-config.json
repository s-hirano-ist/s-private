				{ "type": "test", "section": "💍 テスト", "release-as": "patch" },
				{
					"type": "chore",
					"section": "🤖 その他の変更",
					"release-as": "patch"
				},
				{ "type": "docs", "section": "✏️ ドキュメント", "release-as": "patch" },
				{
					"type": "refactor",
					"section": "💡 リファクタリング",
					"release-as": "patch"
				},
				{ "type": "ci", "section": "🎡 CI関連", "release-as": "patch" },
				{
					"type": "perf",
					"section": "⚡️ パフォーマンス向上",
					"release-as": "patch"
				},
				{
					"type": "contents",
					"section": "💄 コンテンツ",
					"release-as": "patch"
				}
			]
		}
	}
