{
	"name": "@s-hirano-ist/s-notification",
	"version": "1.18.0",
	"description": "Notification service for s-private application - Pushover integration",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./index.ts",
			"default": "./index.ts"
		}
	},
	"publishConfig": {
		"access": "public",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"main": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsc && tsc-esm-fix --target dist",
		"prepublishOnly": "pnpm run build",
		"clean": "rm -rf dist",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"notification",
		"pushover",
		"typescript"
	],
	"author": "",
	"license": "AGPL-3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/s-hirano-ist/s-private.git",
		"directory": "packages/notification"
	},
	"devDependencies": {
		"tsc-esm-fix": "3.1.2",
		"typescript": "5.9.3"
	}
}
