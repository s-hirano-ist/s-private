{
	"name": "@s-hirano-ist/s-search",
	"version": "1.14.0",
	"description": "RAG search library with Qdrant and embedding API client",
	"type": "module",
	"exports": {
		"./config": {
			"types": "./src/config.ts",
			"import": "./src/config.ts"
		},
		"./embedding": {
			"types": "./src/embedding.ts",
			"import": "./src/embedding.ts"
		},
		"./embedding-client": {
			"types": "./src/embedding-client.ts",
			"import": "./src/embedding-client.ts"
		},
		"./qdrant-client": {
			"types": "./src/qdrant-client.ts",
			"import": "./src/qdrant-client.ts"
		},
		"./chunker": {
			"types": "./src/chunker.ts",
			"import": "./src/chunker.ts"
		},
		"./ingest": {
			"types": "./src/ingest.ts",
			"import": "./src/ingest.ts"
		}
	},
	"files": [
		"src",
		"dist",
		"README.md",
		"LICENSE"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/s-hirano-ist/s-private.git",
		"directory": "packages/search"
	},
	"license": "AGPL-3.0",
	"author": "s-hirano-ist",
	"keywords": [
		"rag",
		"search",
		"qdrant",
		"embedding"
	],
	"scripts": {
		"build": "tsc",
		"clean": "rm -rf dist",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "pnpm run build"
	},
	"dependencies": {
		"@qdrant/js-client-rest": "1.16.2"
	},
	"optionalDependencies": {
		"@huggingface/transformers": "3.8.1"
	},
	"devDependencies": {
		"@types/node": "24.10.9",
		"typescript": "5.9.3"
	}
}
