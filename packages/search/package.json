{
	"name": "@s-hirano-ist/s-search",
	"version": "1.14.0",
	"description": "RAG search library with Qdrant and embedding API client",
	"type": "module",
	"exports": {
		"./config": "./src/config.ts",
		"./embedding": "./src/embedding.ts",
		"./embedding-client": "./src/embedding-client.ts",
		"./qdrant-client": "./src/qdrant-client.ts",
		"./chunker": "./src/chunker.ts",
		"./ingest": "./src/ingest.ts"
	},
	"publishConfig": {
		"access": "public",
		"exports": {
			"./config": {
				"types": "./dist/config.d.ts",
				"import": "./dist/config.js"
			},
			"./embedding": {
				"types": "./dist/embedding.d.ts",
				"import": "./dist/embedding.js"
			},
			"./embedding-client": {
				"types": "./dist/embedding-client.d.ts",
				"import": "./dist/embedding-client.js"
			},
			"./qdrant-client": {
				"types": "./dist/qdrant-client.d.ts",
				"import": "./dist/qdrant-client.js"
			},
			"./chunker": {
				"types": "./dist/chunker.d.ts",
				"import": "./dist/chunker.js"
			},
			"./ingest": {
				"types": "./dist/ingest.d.ts",
				"import": "./dist/ingest.js"
			}
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/s-hirano-ist/s-private.git",
		"directory": "packages/search"
	},
	"license": "AGPL-3.0",
	"author": "s-hirano-ist",
	"keywords": [
		"rag",
		"search",
		"qdrant",
		"embedding"
	],
	"scripts": {
		"build": "tsc && tsc-esm-fix --target dist",
		"clean": "rm -rf dist",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "pnpm run build"
	},
	"dependencies": {
		"@qdrant/js-client-rest": "1.16.2"
	},
	"optionalDependencies": {
		"@huggingface/transformers": "3.8.1"
	},
	"devDependencies": {
		"@types/node": "24.10.9",
		"tsc-esm-fix": "3.1.2",
		"typescript": "5.9.3"
	}
}
