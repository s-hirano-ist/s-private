{
	"name": "@s-hirano-ist/s-ui",
	"version": "1.1.5",
	"description": "UI component library for s-private application",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./index.ts",
			"default": "./index.ts"
		},
		"./ui/*": "./ui/*.tsx",
		"./forms/*": "./forms/**/*.tsx",
		"./display/*": "./display/**/*.tsx",
		"./layouts/*": "./layouts/**/*.tsx",
		"./providers/*": "./providers/*.tsx",
		"./hooks/*": "./hooks/*.ts",
		"./utils/*": "./utils/*.ts",
		"./globals.css": "./globals.css"
	},
	"publishConfig": {
		"access": "public",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"./ui/*": {
				"types": "./dist/ui/*.d.ts",
				"default": "./dist/ui/*.js"
			},
			"./forms/*": {
				"types": "./dist/forms/*.d.ts",
				"default": "./dist/forms/*.js"
			},
			"./display/*": {
				"types": "./dist/display/*.d.ts",
				"default": "./dist/display/*.js"
			},
			"./layouts/*": {
				"types": "./dist/layouts/*.d.ts",
				"default": "./dist/layouts/*.js"
			},
			"./providers/*": {
				"types": "./dist/providers/*.d.ts",
				"default": "./dist/providers/*.js"
			},
			"./hooks/*": {
				"types": "./dist/hooks/*.d.ts",
				"default": "./dist/hooks/*.js"
			},
			"./utils/*": {
				"types": "./dist/utils/*.d.ts",
				"default": "./dist/utils/*.js"
			},
			"./globals.css": "./globals.css"
		},
		"main": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"files": [
		"dist",
		"globals.css",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsc",
		"prepublishOnly": "pnpm run build",
		"clean": "rm -rf dist"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/s-hirano-ist/s-private.git",
		"directory": "packages/ui"
	},
	"license": "AGPL-3.0",
	"dependencies": {
		"@tailwindcss/typography": "0.5.19",
		"class-variance-authority": "0.7.1",
		"clsx": "2.1.1",
		"cmdk": "1.1.1",
		"lucide-react": "0.556.0",
		"next": "15.5.9",
		"next-themes": "0.4.6",
		"radix-ui": "1.4.3",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"react-markdown": "10.1.0",
		"react-syntax-highlighter": "16.1.0",
		"remark-gfm": "4.0.1",
		"sonner": "2.0.7",
		"tailwind-merge": "3.4.0",
		"vaul": "1.1.2"
	},
	"devDependencies": {
		"@types/react": "19.2.7",
		"@types/react-dom": "19.2.3",
		"@types/react-syntax-highlighter": "15.5.13",
		"postcss": "8.5.6",
		"typescript": "5.9.3"
	}
}
