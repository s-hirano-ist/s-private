{
	"name": "s-private-app",
	"version": "9.5.2",
	"author": "s-hirano-ist",
	"description": "Private pages and admin tools for s-hirano-ist.",
	"type": "module",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"analyze": "ANALYZE=true pnpm build",
		"start": "next start",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"lint:inspector": "config-inspector --config eslint.config.ts",
		"test": "vitest",
		"prisma:generate": "prisma generate --no-engine --no-hints",
		"prisma:migrate": "prisma migrate dev",
		"prisma:deploy": "prisma migrate deploy",
		"prisma:studio": "prisma studio",
		"postinstall": "pnpm prisma:generate",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build -o .storybook-static",
		"license:summary": "license-checker-rseidelsohn --summary> library-license.txt",
		"knip": "knip",
		"deps:check": "depcruise src --config .dependency-cruiser.cjs  --include-only '^src'",
		"deps:graph": "depcruise src --config .dependency-cruiser.cjs --include-only '^src' --output-type dot | dot -T svg > dependency-graph.svg",
		"deps:circular": "depcruise src --config .dependency-cruiser.cjs  --include-only '^src' --output-type text --focus-depth 0"
	},
	"dependencies": {
		"@prisma/client": "6.16.2",
		"@prisma/extension-accelerate": "2.0.2",
		"@radix-ui/react-icons": "1.3.2",
