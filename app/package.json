		"@eslint/config-inspector": "1.3.0",
		"@eslint/eslintrc": "3.3.1",
		"@next/bundle-analyzer": "15.5.3",
		"@storybook/addon-a11y": "9.1.7",
		"@storybook/addon-actions": "9.0.8",
