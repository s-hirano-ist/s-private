{
	"name": "s-private",
	"version": "9.5.2",
	"author": "s-hirano-ist",
	"description": "Private pages and admin tools for s-hirano-ist.",
	"packageManager": "pnpm@10.17.0",
	"type": "module",
	"scripts": {
		"dev": "pnpm --recursive dev",
		"build": "pnpm --recursive build",
		"clean": "rm -rf app/node_modules app/.next app/dist packages/*/node_modules packages/*/dist",
		"biome:ci": "biome ci .",
		"biome:fix": "biome check --write .",
		"lint": "pnpm --recursive lint",
		"lint:fix": "pnpm --recursive lint:fix",
		"test": "pnpm --recursive test",
		"license:summary": "pnpm --recursive license:summary",
		"security": "pnpm audit",
		"jscpd": "jscpd -p app/src/**/*.{ts,tsx}",
		"knip": "pnpm --filter s-private-app knip",
		"storybook": "pnpm --filter s-private-app storybook",
		"storybook:build": "pnpm --filter s-private-app storybook:build",
		"deps:check": "pnpm -r deps:check",
		"deps:circular": "pnpm -r deps:circular",
		"deps:graph": "pnpm -r deps:graph"
	},
	"dependencies": {},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"jscpd": "4.0.5",
		"playwright": "1.56.0"
	}
}
