{
	"name": "s-private",
	"version": "9.5.1",
	"author": "s-hirano-ist",
	"description": "Private pages and admin tools for s-hirano-ist.",
	"packageManager": "pnpm@10.15.1",
	"type": "module",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"lint:inspector": "config-inspector --config eslint.config.ts",
		"biome:ci": "biome ci .",
		"biome:fix": "biome check --write .",
		"test": "vitest",
		"prisma:generate": "prisma generate --no-engine --no-hints",
		"prisma:migrate": "prisma migrate dev",
		"prisma:deploy": "prisma migrate deploy",
		"prisma:studio": "prisma studio",
		"postinstall": "pnpm prisma:generate",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build -o .storybook-static",
		"license:summary": "license-checker-rseidelsohn --summary> library-license.txt",
		"security": "pnpm audit",
		"analyze": "ANALYZE=true pnpm build",
		"knip": "knip",
		"jscpd": "jscpd -p src/**/*.{ts,tsx}",
		"deps:check": "depcruise src --config .dependency-cruiser.cjs  --include-only '^src'",
		"deps:graph": "depcruise src --config .dependency-cruiser.cjs --include-only '^src' --output-type dot | dot -T svg > dependency-graph.svg",
		"deps:circular": "depcruise src --config .dependency-cruiser.cjs  --include-only '^src' --output-type text --focus-depth 0"
	},
	"dependencies": {
		"@prisma/client": "6.15.0",
		"@prisma/extension-accelerate": "2.0.2",
		"@radix-ui/react-icons": "1.3.2",
		"@sentry/nextjs": "10.10.0",
		"@t3-oss/env-nextjs": "0.13.8",
		"@tailwindcss/typography": "0.5.16",
		"@vercel/analytics": "1.5.0",
		"@vercel/speed-insights": "1.2.0",
		"class-variance-authority": "0.7.1",
		"clsx": "2.1.1",
		"cmdk": "1.1.1",
		"lucide-react": "0.542.0",
		"minio": "8.0.5",
		"next": "15.5.2",
		"next-auth": "5.0.0-beta.29",
		"next-intl": "4.3.6",
		"next-themes": "0.4.6",
		"pino": "9.9.4",
		"prisma": "6.15.0",
		"radix-ui": "1.4.3",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"react-markdown": "10.1.0",
		"react-syntax-highlighter": "15.6.6",
		"server-only": "0.0.1",
		"sharp": "0.34.3",
		"sonner": "2.0.7",
		"tailwind-merge": "3.3.1",
		"uuid": "12.0.0",
		"vaul": "1.1.2",
		"yet-another-react-lightbox": "3.25.0",
		"zod": "4.1.5"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.3",
		"@chromatic-com/storybook": "4.1.1",
		"@eslint/config-inspector": "1.2.0",
		"@eslint/eslintrc": "3.3.1",
		"@next/bundle-analyzer": "15.5.2",
		"@storybook/addon-a11y": "9.1.5",
		"@storybook/addon-actions": "9.0.8",
		"@storybook/addon-backgrounds": "9.0.8",
		"@storybook/addon-controls": "9.0.8",
		"@storybook/addon-docs": "9.1.5",
		"@storybook/addon-links": "9.1.5",
		"@storybook/addon-measure": "9.0.8",
		"@storybook/addon-onboarding": "9.1.5",
		"@storybook/addon-outline": "9.0.8",
		"@storybook/addon-themes": "9.1.5",
		"@storybook/addon-toolbars": "9.0.8",
		"@storybook/addon-viewport": "9.0.8",
		"@storybook/addon-vitest": "9.1.5",
		"@storybook/nextjs-vite": "9.1.5",
		"@tailwindcss/postcss": "4.1.13",
		"@testing-library/jest-dom": "6.8.0",
		"@testing-library/react": "16.3.0",
		"@testing-library/user-event": "14.6.1",
		"@types/node": "22.18.1",
		"@types/react": "19.1.12",
		"@types/react-dom": "19.1.9",
		"@types/react-syntax-highlighter": "15.5.13",
		"@vitest/browser": "3.2.4",
		"@vitest/coverage-v8": "3.2.4",
		"@vitest/eslint-plugin": "1.3.9",
		"dependency-cruiser": "17.0.1",
		"eslint": "9.35.0",
		"eslint-config-next": "15.5.2",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-storybook": "9.1.5",
		"eslint-plugin-tailwindcss": "4.0.0-beta.0",
		"jscpd": "4.0.5",
		"jsdom": "26.1.0",
		"knip": "5.63.1",
		"license-checker-rseidelsohn": "4.4.2",
		"playwright": "1.55.0",
		"postcss": "8.5.6",
		"remark-gfm": "4.0.1",
		"storybook": "9.1.5",
		"tailwindcss": "4.1.13",
		"typescript": "5.9.2",
		"typescript-eslint": "8.42.0",
		"vitest": "3.2.4"
	}
}
