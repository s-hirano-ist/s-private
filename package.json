	"author": "s-hirano-ist",
	"description": "Private pages and admin tools for s-hirano-ist.",
	"packageManager": "pnpm@9.15.2",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"lint:mark": "markdownlint-cli2 _markdown/*",
		"lint:mark:fix": "markdownlint-cli2-fix _markdown/*",
		"check:ci": "biome ci *",
		"check:fix": "biome check --write *",
		"test": "vitest",
		"test:e2e": "playwright test",
		"test:storybook": "test-storybook --coverage --coverageDirectory .storybook-coverage",
		"prisma:generate": "prisma generate --no-hints",
		"prisma:migrate": "prisma migrate dev",
		"prisma:deploy": "prisma migrate deploy",
		"prisma:seed": "prisma db seed",
		"prisma:studio": "prisma studio",
		"postinstall": "pnpm prisma:generate",
		"commit": "git-cz",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build -o .storybook-static",
		"license:summary": "license-checker --summary> library-license.txt",
		"security": "pnpm audit",
		"actionlint": "actionlint",
		"ghalint": "ghalint run"
	},
	"prisma": {
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	},
