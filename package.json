{
	"name": "s-private",
	"version": "9.5.2",
	"author": "s-hirano-ist",
	"description": "Private pages and admin tools for s-hirano-ist.",
	"packageManager": "pnpm@10.17.0",
	"type": "module",
	"scripts": {
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"lint:inspector": "config-inspector --config eslint.config.ts",
		"biome:ci": "biome ci .",
		"biome:fix": "biome check --write .",
		"license:summary": "license-checker-rseidelsohn --summary> library-license.txt",
		"security": "pnpm audit",
		"analyze": "ANALYZE=true pnpm build",
		"knip": "knip",
		"jscpd": "jscpd -p src/**/*.{ts,tsx}",
		"deps:check": "depcruise src --config .dependency-cruiser.cjs  --include-only '^src'",
		"deps:graph": "depcruise src --config .dependency-cruiser.cjs --include-only '^src' --output-type dot | dot -T svg > dependency-graph.svg",
		"deps:circular": "depcruise src --config .dependency-cruiser.cjs  --include-only '^src' --output-type text --focus-depth 0"
	},
	"dependencies": {},
	"devDependencies": {
		"@biomejs/biome": "2.2.4"
	}
}
